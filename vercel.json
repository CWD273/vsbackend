{
  "version": 2,
  "rewrites": [
    {
      "source": "/s.json",
      "destination": "/api/s.json"
    }
  ],
  "headers": [
    {
      "source": "/s.json",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=60" },
        { "key": "Content-Type", "value": "application/json" }
      ]
    }
  ],
  "functions": {
    "api/s.json.js": {
      "runtime": "nodejs18.x",
      "memory": 128,
      "maxDuration": 5
    }
  }
}
